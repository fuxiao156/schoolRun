.nav-wrap {
    background: #fff;
    color: #000;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9999999;
}

.nav-title {
    bottom: 0;
    color: #2c2b2b;
    display: inline-flex;
    font-size: 34rpx;
    justify-content: left;
    left: 0;
    margin: auto;
    max-width: 97%;
    overflow: hidden;
    padding: 3%;
    position: absolute;
    right: 0;
    text-align: center;
    text-overflow: ellipsis;
    top: 0;
    white-space: nowrap;
}

.back-pre-view,.nav-title {
    align-items: center;
}

.back-pre-view {
    justify-content: center;
    min-height: 0rpx;
    width: auto;
}

.back-pre-view,.nav-capsule {
    display: flex;
}

.nav-capsule {
    align-items: left;
    background: red;
    height: 100%;
    justify-content: space-between;
    margin-left: 30rpx;
    width: 140rpx;
}

.navbar-v-line {
    background-color: #e5e5e5;
    height: 32rpx;
    width: 1px;
}

.back-home,.back-pre {
    height: 36rpx;
    margin-top: 4rpx;
    padding: 10rpx;
    width: 36rpx;
}

.nav-capsule .back-home {
    height: 50rpx;
    margin-top: 3rpx;
    width: 50rpx;
}