.v1 {
    color: #0094fe;
    font-size: 36rpx;
    margin-bottom: 20rpx;
    padding-left: 30rpx;
}

.helplist,.v1 {
    font-weight: 700;
}

.helplist {
    background: #fff;
    border-bottom: 1rpx solid #eaeaea;
    color: #333;
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx;
    padding-left: 20rpx;
    position: relative;
}

.helplist::after {
    border-right: 1px solid #b7b7b7;
    border-top: 1px solid #b7b7b7;
    content: "  ";
    height: 20rpx;
    position: absolute;
    right: 30rpx;
    text-align: center;
    top: 40rpx;
    transform: rotate(45deg);
    width: 20rpx;
}

.copy {
    color: #0094fe;
    height: 100rpx;
    margin-top: 30rpx;
    text-align: center;
    width: 100%;
}

.foot {
    bottom: 50rpx;
    height: auto;
    position: fixed;
    width: 100%;
    z-index: 999;
}

.foot-but {
    align-items: center;
    background: #0094fe;
    border-radius: 100rpx;
    color: #fff;
    display: flex;
    height: 100rpx;
    justify-content: center;
    margin-left: 25%;
    margin-top: 20rpx;
    width: 50%;
}

.foot-text {
    color: #000;
}

.foot-red-text,.foot-text {
    display: inline-block;
    font-size: 26rpx;
    margin-top: 30rpx;
    text-align: center;
    vertical-align: middle;
    width: 100%;
}

.foot-red-text {
    color: red;
}