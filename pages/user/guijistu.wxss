page {
    background: #f7f7f7;
}

.tab {
    float: left;
    font-size: 30rpx;
    padding: 15rpx 0;
    text-align: center;
    width: 45%;
}

.topTabSwiper {
    zoom: 1;
    background: #fff;
    border-bottom: 0 solid #ccc;
    border-top: 0 solid #ccc;
    font-size: 30rpx;
    font-weight: 400;
    height: 10%;
    padding-left: 10%;
    width: 100%;
    z-index: 10000;
}

.topTabSwiper:after {
    clear: both;
    content: "";
    display: block;
}

.tabBorer {
    border-bottom: 1px solid #ff9801;
    color: #ff9801;
    font-size: 30rpx;
    font-weight: 400;
}

.swiper {
    width: 100%;
}

.swiper_con {
    height: 100%;
    text-align: center;
    width: 100%;
}

.linebg {
    background: #fff;
    border-bottom: 2rpx solid #d6d6d6;
    height: 2rpx;
    width: 100%;
}

.rank-type {
    bottom: 0;
    position: fixed;
    width: 100%;
}

.rank-type .ranklabel {
    display: inline-block;
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    width: 32%;
}

.rank-type .active {
    color: #ff9801;
}

.rank-type .sline {
    background: #f6f6f6;
    display: inline-block;
    height: 40rpx;
    width: 4rpx;
}

.rank-list {
    border-bottom: 1rpx solid #e0e0e0;
    color: #333;
    font-size: 32rpx;
    height: 126rpx;
    line-height: 50rpx;
    padding-top: 20rpx;
    width: 100%;
}

.rank-list .ranknum {
    color: #595959;
    float: left;
    font-size: 36rpx;
    line-height: 100rpx;
    width: 12%;
}

.rank-list .headimg-view {
    float: left;
    padding-top: 6rpx;
    width: 20%;
}

.rank-list .headimg-view .headimg {
    border-radius: 48rpx;
    height: 95rpx;
    width: 95rpx;
}

.level {
    color: #999;
    font-size: 28rpx;
}

.stutentname {
    font-size: 32rpx;
}

.rank-list .name-view {
    float: left;
    font-size: 30rpx;
    text-align: left;
    width: 30%;
}

.rank-list .bbdata-view {
    float: right;
    font-size: 30rpx;
    padding-right: 30rpx;
    text-align: right;
    width: 25%;
}

.date-view {
    align-items: center;
    display: flex;
    flex-direction: row;
    font-size: 28rpx;
    height: 70rpx;
    justify-content: left;
    line-height: 70rpx;
    padding-left: 5%;
    text-align: left;
}

.mapview {
    background: #fff;
    border-radius: 10rpx;
    height: 95%;
    margin: 0 auto;
    width: 96%;
}

.mapview .map {
    height: 70%;
    margin: 0 auto;
    width: 100%;
}

.top-view {
    background: #fff;
    border: 0.5rpx solid #eaeaea;
    height: 300rpx;
    left: 4%;
    margin-top: 40rpx;
    position: fixed;
    width: 92%;
    z-index: 1000;
}

.top-view-1 {
    border-bottom: 1rpx solid #eaeaea;
    height: 120rpx;
    margin: 30rpx auto 0;
    width: 90%;
}

.label1 {
    font-size: 100rpx;
    padding-right: 10rpx;
}

.headview1 {
    height: 100rpx;
    line-height: 100rpx;
    position: relative;
}

.headview1,.view-1 {
    border-bottom: 1px dashed #a9a9a9;
    font-size: 28rpx;
    width: 100%;
}

.view-1 {
    height: 150rpx;
    line-height: 150rpx;
}

.view-1 .view-1-left {
    display: inline-block;
    width: 25%;
}

.view-1 .view-1-right {
    display: inline-block;
    padding-right: 3%;
    text-align: right;
    width: 72%;
}

.top-view-2 {
    height: 120rpx;
    margin: 30rpx auto 0 3%;
    width: 95%;
}

.top-view-box {
    display: inline-block;
    text-align: center;
    width: 33%;
}

.top-view-box view {
    font-size: 42rpx;
    font-weight: 700;
}

.top-view-box label {
    color: #999;
    font-size: 30rpx;
}

.font1 {
    font-size: 50rpx;
    font-weight: 700;
}

.font1::after {
    color: #333;
    content: "km";
    font-size: 24rpx;
}

.font2 {
    color: #ff9800;
}

.stu-img {
    border-radius: 100rpx;
    display: block;
    height: 100rpx;
    left: 30rpx;
    position: absolute;
    top: 10rpx;
    width: 100rpx;
}

.img-wrap {
    padding-top: 42.3333%;
    position: relative;
    width: 100%;
    z-index: 10000;
}

.img {
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
}

.date-txt {
    align-items: center;
    color: #333;
    display: flex;
    flex-direction: row;
    font-size: 28rpx;
    font-weight: 700;
    height: 80rpx;
    justify-content: left;
    line-height: 80rpx;
    margin-top: 20rpx;
    padding-left: 3%;
}

.time-img {
    display: inline-block;
    height: 40rpx;
    padding-right: 10rpx;
    width: 40rpx;
}

.d-bg {
    background: #fff;
    border-radius: 10rpx;
    min-height: 300rpx;
    width: 94%;
}

.d-bg,.gline {
    margin: 0 auto;
}

.gline {
    background: #f7f7f7;
    height: 6rpx;
    width: 98%;
}

.tubiao {
    margin: 30rpx;
}

.bshu {
    font-size: 32rpx;
    font-weight: 700;
}

.pei {
    margin: 30rpx;
}