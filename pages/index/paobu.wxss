.top-view {
    background-color: #fff;
    font-size: 28rpx;
    height: auto;
    position: fixed;
    width: 100%;
    z-index: 100;
}

.top-view-1 {
    display: flex;
    height: 70rpx;
    line-height: 80rpx;
    margin-left: 5%;
    width: 90%;
}

.label1 {
    font-size: 70rpx;
    font-weight: 600;
    width: fit-content;
}

.label1,.label1-km {
    align-items: flex-end;
    display: flex;
}

.label1-km {
    font-size: 30rpx;
    padding-left: 6rpx;
    width: auto;
}

.top-view-2 {
    display: flex;
    height: auto;
    margin-left: 5%;
    text-align: left;
    width: 90%;
}

.top-view-box {
    width: 33%;
}

.error-box,.top-view-box {
    display: inline-block;
    text-align: center;
}

.error-box {
    color: #999;
    font-size: 30rpx;
    width: 60%;
}

.text-min {
    align-items: center;
    border-radius: 70rpx;
    display: flex;
    font-size: 28rpx;
    height: auto;
    justify-content: center;
    position: absolute;
    right: 10rpx;
    text-align: center;
    top: 0rpx;
    width: 90rpx;
    z-index: 1100;
}

.vido-off,.vido-open {
    align-items: center;
    border: 1px solid #999;
    border-radius: 70rpx;
    display: flex;
    height: 70rpx;
    justify-content: center;
    position: absolute;
    right: 30rpx;
    text-align: center;
    top: 40rpx;
    width: 70rpx;
    z-index: 1100;
}

.top-view-head {
    font-size: 42rpx;
    font-weight: 600;
}

.top-view-end {
    color: #999;
    font-size: 30rpx;
}

.pb-btn-vew {
    bottom: 30rpx;
    position: fixed;
    width: 100%;
    z-index: 999;
}

.pb-btn-vew .btn-endstop {
    margin-left: 4%;
}

.pb-btn-vew .btn-end,.pb-btn-vew .btn-endstop {
    background: #ee7200;
    border-radius: 48rpx;
    color: #fff;
    display: inline-block;
    font-size: 48rpx;
    font-weight: 400;
    height: 96rpx;
    letter-spacing: 6pt;
    line-height: 96rpx;
    position: relative;
    text-align: center;
    width: 44%;
}

.pb-btn-vew .btn-end {
    margin-left: 33%;
}

.pb-btn-vew .ctxt {
    font-size: 28rpx;
    left: 30rpx;
    letter-spacing: 1pt;
    opacity: 1;
    position: absolute;
    top: 30rpx;
}

.pb-btn-vew .btn-stop,.pb-btn-vew .ctxt {
    display: inline-block;
    font-weight: 400;
    text-align: center;
}

.pb-btn-vew .btn-stop {
    background: #0867c1;
    border-radius: 48rpx;
    color: #fff;
    font-size: 48rpx;
    height: 96rpx;
    letter-spacing: 6pt;
    line-height: 96rpx;
    margin-left: 4%;
    position: relative;
    width: 44%;
}

.set-view {
    position: fixed;
    right: 40rpx;
    top: 400rpx;
    z-index: 1000;
}

.set-view view {
    line-height: 100rpx;
}

.set-view .iconimg {
    height: 60rpx;
    width: 60rpx;
}

.endicon {
    background: #fff;
    height: 30rpx;
    top: 35rpx;
    width: 30rpx;
}

.endicon,.zticon {
    position: absolute;
    right: 60rpx;
}

.zticon {
    height: 40rpx;
    top: 30rpx;
    width: 40rpx;
}

.btn-end2 {
    background: #eaeaea;
}

.off-on-btn,.on-off-btn {
    height: 40rpx;
    position: relative;
    width: 100%;
}

.on-off-btn::after {
    bottom: 0rpx;
    transform: rotate(-45deg);
}

.off-on-btn::after,.on-off-btn::after {
    border-color: #a9a9a9;
    border-style: solid;
    border-width: 3rpx 3rpx 0 0;
    content: " ";
    height: 20rpx;
    position: absolute;
    right: 48%;
    width: 20rpx;
}

.off-on-btn::after {
    bottom: 14rpx;
    transform: rotate(135deg);
}

