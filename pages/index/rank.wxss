page {
    background: #fff;
}

.tab {
    float: left;
    font-size: 30rpx;
    padding: 15rpx 0;
    text-align: center;
    width: 30%;
}

.topTabSwiper {
    zoom: 1;
    background: #fff;
    border-bottom: 0 solid #ccc;
    border-top: 0 solid #ccc;
    font-size: 30rpx;
    font-weight: 400;
    padding-left: 10%;
    position: fixed;
    width: 100%;
    z-index: 10000;
}

.topTabSwiper:after {
    clear: both;
    content: "";
    display: block;
}

.tabBorer {
    border-bottom: 1px solid #ff9801;
    color: #ff9801;
    font-size: 30rpx;
    font-weight: 400;
}

.swiper {
    width: 100%;
}

.swiper_con {
    background: none;
    height: 100%;
    margin-top: 60rpx;
    padding: 0rpx 0;
    text-align: center;
    width: 100%;
}

.linebg {
    border-bottom: 2rpx solid #d6d6d6;
    height: 2rpx;
}

.linebg,.rank-type {
    background: #fff;
    width: 100%;
}

.rank-type {
    bottom: 0;
    position: fixed;
}

.rank-type .ranklabel {
    display: inline-block;
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    width: 32%;
}

.rank-type .active {
    color: #ff9801;
}

.rank-type .sline {
    background: #f6f6f6;
    display: inline-block;
    height: 40rpx;
    width: 4rpx;
}

.rank-list {
    border-bottom: 1rpx solid #e0e0e0;
    color: #333;
    font-size: 32rpx;
    height: 126rpx;
    line-height: 50rpx;
    padding-top: 20rpx;
    width: 100%;
}

.rank-list .ranknum {
    color: #595959;
    float: left;
    font-size: 36rpx;
    font-weight: 600;
    line-height: 1fantasy;
    width: 12%;
}

.rank-list .headimg-view {
    float: left;
    padding-top: 6rpx;
    width: 20%;
}

.rank-list .headimg-view .headimg {
    border-radius: 48rpx;
    height: 95rpx;
    width: 95rpx;
}

.level {
    color: #999;
    font-size: 28rpx;
}

.stutentname {
    font-size: 32rpx;
}

.gl1 {
    font-size: 50rpx;
    font-weight: 600;
    letter-spacing: -3rpx;
}

.gl3,.gl4 {
    color: #c8cdcf;
    font-size: 30rpx;
}

.gl4 {
    font-weight: 700rpx;
}

.rank-list .name-view {
    float: left;
    font-size: 30rpx;
    text-align: left;
    width: 30%;
}

.rank-list .bbdata-view {
    float: right;
    font-size: 30rpx;
    padding-right: 30rpx;
    text-align: right;
    width: 30%;
}